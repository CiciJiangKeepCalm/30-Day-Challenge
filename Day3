 
------
using System;
using System.Collections.Generic;
using System.IO;
using System.Linq;
class CandidateCode {
   static void Main(String[] args) {
    string a = Console.ReadLine();
string b = Console.ReadLine();
string c = Console.ReadLine();
int p= Int32.Parse(a);
int i= Int32.Parse(b);
int y= Int32.Parse(c);
 
int result = ((p*i*y)/100);       
Console.WriteLine(result);
 
   }
}
